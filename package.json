{
  "name": "brophy-react-starter",
  "version": "1.0.1",
  "description": "A starter project for a full-stack react app.",
  "main": "webpack.config.js",
  "engines": {
    "node": "8.4.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "start:dev": "env NODE_ENV=development env ACCESS_KEY=a791ca62357f470d913d88d353e351e0 node server/server.js",
    "start:debug": "env NODE_ENV=development env ACCESS_KEY=a791ca62357f470d913d88d353e351e0 node $NODE_DEBUG_OPTION server/server.js",
    "build:dev": "rimraf ./dist && webpack --config webpack.config.js",
    "build:prod": "rm -rf ./dist && webpack --config webpack.config.production.js",
    "heroku-postbuild": "npm rebuild node-sass && npm run build:prod",
    "knex": "knex",
    "migrate:latest:test": "env NODE_ENV=test knex migrate:latest",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback:test": "env NODE_ENV=test knex migrate:rollback",
    "migrate:rollback": "knex migrate:rollback",
    "nf": "nf start",
    "resetdb": "npm run migrate:rollback && npm run migrate:latest && npm run seed",
    "resetdb:test": "npm run migrate:rollback:test && npm run migrate:latest:test && npm run seed:test",
    "seed:test": "env NODE_ENV=test knex seed:run",
    "seed": "knex seed:run",
    "test:server": "mocha test/**/*.test.js --env test",
    "test": "npm run test:server",
    "test:push": "npm test && git push origin HEAD"
  },
  "keywords": [
    "React",
    "React-Router 4",
    "MobX",
    "Webpack 2",
    "ES6",
    "ES7"
  ],
  "author": "Bryan Nrophy",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.1.3",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.1.0-beta.8"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "body-parser": "^1.17.1",
    "bookshelf": "^0.10.3",
    "bookshelf-camelcase": "^1.1.4",
    "bootstrap-sass": "^3.3.7",
    "chart.js": "^2.7.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "faker": "^3.1.0",
    "http-server": "^0.9.0",
    "humps": "^2.0.0",
    "include-media": "^1.4.9",
    "joi": "^10.2.2",
    "jquery": "^3.2.1",
    "knex": "^0.12.8",
    "lazy-route": "^1.0.7",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "material-ui": "^1.0.0-beta.23",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-react-devtools": "^4.2.6",
    "moment": "^2.15.1",
    "morgan": "^1.8.1",
    "node-mailer": "^0.1.1",
    "node-mailjet": "^3.2.1",
    "nodemailer": "^4.4.1",
    "package": "^1.0.1",
    "pg": "^6.1.5",
    "react": "^15.6.2",
    "react-chartjs": "git://github.com/reactjs/react-chartjs.git#chartjs-v2",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^15.6.2",
    "react-router-dom": "^4.0.0",
    "survey-react": "^0.98.3",
    "surveyjs-editor": "^0.98.3",
    "typeface-roboto": "0.0.45",
    "whatwg-fetch": "^1.0.0"
  },
  "repository": {
    "url": "https://github.com/keyopsapp/keyops-pilot.git",
    "type": "git"
  }
}
